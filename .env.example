# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY="your_firebase_api_key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="your_project.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id""
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="your_project.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="your_sender_id"
NEXT_PUBLIC_FIREBASE_APP_ID="your_app_id"

# Google Maps API
# Prefer a server-only API key to avoid exposing it to the browser.
# Add a server-only key as GOOGLE_MAPS_API_KEY and the API routes will use it.
# If you must expose it to client (not recommended), keep NEXT_PUBLIC_GOOGLE_MAPS_API_KEY.
GOOGLE_MAPS_API_KEY="your_google_maps_server_key"
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your_google_maps_api_key" # optional, not recommended

# Telegram BOT + Chat ID
NEXT_PUBLIC_TELEGRAM_BOT_TOKEN="your_telegram_bot_token"
NEXT_PUBLIC_CHAT_ID="your_chat_id"

